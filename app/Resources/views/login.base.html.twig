<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        {% stylesheets 'bundles/lunauser/css/*' 'bundles/lunaapplication/css/*' 'bundles/lunareception/css/*' '@bootstrap_less' combine=true %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
        {% endstylesheets %}
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>

{% block header %}
    {{ include('::header.html.twig') }}
{% endblock %}

{% block content %}{% endblock %}

<footer class="col-md-12" id="footer_wrap">
    {% block footer %}
        {{ include('::footer.html.twig') }}
    {% endblock %}
</footer>

{% block javascripts %}
    {% javascripts '@jquery_js' '@bootstrap_js' filter='?yui_js' combine=false %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>
